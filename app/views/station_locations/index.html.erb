  <script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
  <script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>



<div style='width: 800px;'>
  <div id="map" style='width: 800px; height: 400px;'></div>
</div>

<h1>Listing station_locations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Gmaps</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @station_locations.each do |station_location| %>
      <tr>
        <td><%= station_location.name %></td>
        <td><%= station_location.address1 %></td>
        <td><%= station_location.address2 %></td>
        <td><%= station_location.city %></td>
        <td><%= station_location.state %></td>
        <td><%= station_location.zip %></td>
        <td><%= station_location.latitude %></td>
        <td><%= station_location.longitude %></td>
        <td><%= station_location.gmaps %></td>
        <td><%= link_to 'Show', station_location %></td>
        <td><%= link_to 'Edit', edit_station_location_path(station_location) %></td>
        <td><%= link_to 'Destroy', station_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Station location', new_station_location_path %>


<script type="text/javascript">
handler = Gmaps.build('Google');
handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){
  markers = handler.addMarkers(<%=raw @hash.to_json %>);
  handler.bounds.extendWith(markers);
  handler.fitMapToBounds();
});

</script>